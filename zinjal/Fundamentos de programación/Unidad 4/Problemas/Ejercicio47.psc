Algoritmo Ejercicio47
	Dimension A[6,4]
	Para f<- 1 Hasta 6 Hacer
		Para c<-1 Hasta 4 Hacer
			Leer A[f,c]
		FinPara
	FinPara
	Escribir "Elemento fila 2 columna 4 " , A[2,4];
	
	Escribir "Tercer columna ";
	Para f<- 1 Hasta 6 Hacer
		Escribir A[f, 3];
	FinPara
	
	Escribir "Quinta fila ";
	Para c<- 1 Hasta 4 Hacer
		Escribir A[5, c];
	FinPara
FinAlgoritmo
